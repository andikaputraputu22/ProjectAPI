<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/listHargaFragment">

    <fragment
        android:id="@+id/listHargaFragment"
        android:name="com.mobile.projectapi.ui.ListHargaFragment"
        android:label="fragment_list_harga"
        tools:layout="@layout/fragment_list_harga" >
        <action
            android:id="@+id/action_listHargaFragment_to_addListFragment"
            app:destination="@id/addListFragment" />
    </fragment>
    <fragment
        android:id="@+id/addListFragment"
        android:name="com.mobile.projectapi.ui.AddListFragment"
        android:label="fragment_add_list"
